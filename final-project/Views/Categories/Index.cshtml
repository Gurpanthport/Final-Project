
@using final_project.Models
@model IEnumerable<CategoryType>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Browse Menu Types:</h2>
<p>
    Select from @Model.Count() different Category Types:
</p>
<ul>
    @foreach (CategoryType Items in Model)
    {
        <li>@Html.ActionLink(Items.Name, "Browse", new { CategoryType = Items.Name })</li>
    }
</ul>
